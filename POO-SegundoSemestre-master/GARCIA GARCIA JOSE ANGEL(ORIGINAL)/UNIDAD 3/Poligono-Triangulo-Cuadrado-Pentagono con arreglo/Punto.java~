public class Punto
{
    private double x,y;
    public Punto(double x, double y){
    setx(x);
    sety(y);
    }
    public void setx(double x){
    this.x=x;
    }
    public int getx(){
    return x;
    }
    public void sety(double y){
    this.y=y;
    }
    public double gety(){
    return y;
    }
}
